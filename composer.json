{
    "name": "arbory/omnipay-worldline",
    "description": "Worldline omnipay package",
    "type": "library",
    "authors": [
    ],
    "autoload": {
      "psr-4": { "Omnipay\\Worldline\\" : "src/" }
    },
    "require": {
      "omnipay/common": "~3.0",
        "php-http/guzzle7-adapter": "^1.0"
    },
    "require-dev": {
      "php": "^8.0|^8.1",
      "omnipay/tests": "^4",
      "squizlabs/php_codesniffer": "^3",
      "phpro/grumphp": "^1",
      "php-coveralls/php-coveralls": "^2"
    },
    "config": {
        "allow-plugins": {
            "phpro/grumphp": true
        }
    }
}
